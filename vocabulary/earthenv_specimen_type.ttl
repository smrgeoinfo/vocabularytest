  skos:inScheme esmat:essampletype ;
  skos:prefLabel "Material Captured in Filter"@en ;
.
esmat:mechanicalfraction
  rdf:type skos:Concept ;
  dcterm:source "this vocabulary" ;
  rdfs:isDefinedBy esmat:essampletype ;

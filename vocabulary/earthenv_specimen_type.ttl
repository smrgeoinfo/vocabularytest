  skos:prefLabel "Meteorite"@en ;
.
esmat:mineralseparate
  rdf:type skos:Concept ;
  dcterm:source "this vocabulary" ;
  rdfs:isDefinedBy esmat:essampletype ;
  rdfs:label "Mineral separate"@en ;
  skos:broader spec:analyticalpreparation ;
  skos:broader spec:genericaggregation ;
  skos:definition "an aggregation of particles of the same mineral extracted and concentrated from a rock."@en ;
  skos:inScheme esmat:essampletype ;
  skos:prefLabel "Mineral separate"@en ;
.

lith:tephra
  rdf:type rdfs:Resource ;
  rdf:type skos:Concept ;
  dcterms:source "Hallsworth and Knox 1999; LeMaitre et al. 2002"@en ;
  skos:broader lith:natural_unconsolidated_material ;
  skos:broader lith:pyroclastic_material ;
  skos:definition "Unconsolidated pyroclastic material in which greater than 75 percent of the fragments are deposited as a direct result of volcanic processes and the deposit has not been reworked by epiclastic processes. Includes ash, lapilli tephra, bomb tephra, block tephra and unconsolidated agglomerate."@en ;
  skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/simplelithology> ;
  skos:narrower lith:ash_and_lapilli ;
  skos:narrower lith:ash_breccia_bomb_or_block_tephra ;
  skos:prefLabel "tephra"@en ;

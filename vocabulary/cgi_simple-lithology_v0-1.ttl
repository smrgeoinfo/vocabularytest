  rdf:type rdfs:Resource ;
  rdf:type skos:Concept ;
  dcterms:source "LeMaitre et al. 2002"@en ;
  skos:broader lith:granitoid ;
  skos:definition "Granitic rock that has a plagioclase to total feldspar ratio less than 0.1. QAPF field 2."@en ;
  skos:example "alaskite"@en ;
  skos:example "peralkaline granite"@en ;
  skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/simplelithology> ;
  skos:prefLabel "alkali feldspar granite"@en ;
.

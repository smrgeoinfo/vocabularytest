  rdf:type rdfs:Resource ;
  rdf:type skos:Concept ;
  dcterms:source "LeMaitre et al. 2002"@en ;
  skos:broader lith:granitoid ;
  skos:definition "Granitoid consisting of quartz and intermediate plagioclase, usually with biotite and amphibole. Includes rocks defined modally in QAPF field 5; ratio of plagioclase to total feldspar is greater than 0.9."@en ;
  skos:example "plagiogranite"@en ;
  skos:example "trondhjemite"@en ;
  skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/simplelithology> ;
  skos:prefLabel "tonalite"@en ;
.
